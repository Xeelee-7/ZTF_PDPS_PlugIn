<tx:TxWindow xmlns:UserControls="clr-namespace:Tecnomatix.Engineering.Ui.WPF.UserControls;assembly=Tecnomatix.Engineering.Ui"  
             x:Class="ZTF_PDPS_Command.CylinderCreatForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ZTF_PDPS_Command"
             xmlns:tx="clr-namespace:Tecnomatix.Engineering.Ui.WPF;assembly=Tecnomatix.Engineering.Ui"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <tx:TxObjectGridControl x:Name="elemList" Height="300" Width="300" Margin="10" ListenToPick="True" ValidatorType="Always" >
                <tx:TxObjectGridControl.Columns>
                    <DataGridTextColumn Header="obj"  Width="310" Binding="{Binding Name}"/>
                </tx:TxObjectGridControl.Columns>
            </tx:TxObjectGridControl>
            <tx:TxObjEditBoxControl Width="300" Margin="10" ListenToPick="True" />

        </StackPanel>

    </Grid>
</tx:TxWindow>
